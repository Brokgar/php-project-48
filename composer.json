{
  "name": "your-vendor/gendiff",
  "description": "PHP library for comparing configuration files and generating diffs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Max Kavun",
      "email": "max.kavun@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Hexlet\\Gendiff\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "bin": [
    "bin/gendiff"
  ],
  "require": {
    "php": "^8.0",
    "symfony/yaml": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.8"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
